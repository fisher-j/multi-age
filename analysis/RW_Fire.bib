  langid = {english},
  keywords = {lidar,plant model,stand,tls},
  file = {C\:\\Users\\walki\\Zotero\\storage\\MKHV5FKG\\Calders et al_2018_Realistic Forest Stand Reconstruction from Terrestrial LiDAR for Radiative.pdf}
}

