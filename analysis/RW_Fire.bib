@article{atchleyEffectsFuelSpatial2021,
  title = {Effects of Fuel Spatial Distribution on Wildland Fire Behaviour},
  author = {Atchley, Adam L. and Linn, Rodman and Jonko, Alex and Hoffman, Chad and Hyman, Jeffrey D. and Pimont, Francois and Sieg, Carolyn and Middleton, Richard S.},
