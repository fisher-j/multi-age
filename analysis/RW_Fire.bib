@article{rodriguezysilvaAssessmentCrownFire2017,
  title = {Assessment of Crown Fire Initiation and Spread Models in {{Mediterranean}} Conifer Forests by Using Data from Field and Laboratory Experiments},
  author = {{Rodr{\'i}guez y Silva}, Francisco and Guijarro, Mercedes and Madrigal, Javier and Jim{\'e}nez, Enrique and Molina, Juan R. and Hernando, Carmen and V{\'e}lez, Ricardo and Vega, Jose A.},
  year = {2017},
  month = oct,
  journal = {Forest Systems},
  volume = {26},
