@article{finneyNeedTheoryWildland2012,
  title = {On the Need for a Theory of Wildland Fire Spread},
  author = {Finney, Mark A. and Cohen, Jack D. and McAllister, Sara S. and Jolly, W. Matt and Finney, Mark A. and Cohen, Jack D. and McAllister, Sara S. and Jolly, W. Matt},
  year = {2012},
