@article{linnQUICfireFastrunningSimulation2020,
  title = {{{QUIC-fire}}: A Fast-Running Simulation Tool for Prescribed Fire Planning},
  shorttitle = {{{QUIC-fire}}},
  author = {Linn, R. R. and Goodrick, S. L. and Brambilla, S. and Brown, M. J. and Middleton, R. S. and O'Brien, J. J. and Hiers, J. K.},
  year = {2020},
  month = mar,
