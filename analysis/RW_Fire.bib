@article{barFireEffectsTree2019,
  title = {Fire Effects on Tree Physiology},
  author = {B{\"a}r, Andreas and Michaletz, Sean T. and Mayr, Stefan},
  year = {2019},
  journal = {New Phytologist},
  volume = {223},
  number = {4},
