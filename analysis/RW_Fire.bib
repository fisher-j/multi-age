@article{keaneEvaluatingPerformanceMapping2013,
  title = {Evaluating the Performance and Mapping of Three Fuel Classification Systems Using {{Forest Inventory}} and {{Analysis}} Surface Fuel Measurements},
  author = {Keane, Robert E. and Herynk, Jason M. and Toney, Chris and Urbanski, Shawn P. and Lutes, Duncan C. and Ottmar, Roger D.},
  year = {2013},
  month = oct,
  journal = {Forest Ecology and Management},
  volume = {305},
