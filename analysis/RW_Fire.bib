@article{bilgiliModelingSurfaceFuels2019,
  title = {Modeling Surface Fuels Moisture Content in {{Pinus}} Brutia Stands},
  author = {Bilgili, Ertugrul and Coskuner, Kadir Alperen and Usta, Yetkin and Goltas, Merih},
  year = {2019},
  month = apr,
  journal = {Journal of Forestry Research},
  volume = {30},
  number = {2},
  pages = {577--587},
  issn = {1993-0607},
  doi = {10.1007/s11676-018-0702-x},
  url = {https://doi.org/10.1007/s11676-018-0702-x},
  urldate = {2022-09-27},
  abstract = {Fuel moisture content is an important variable for forest fires because it affects fuel ignition and fire behavior. In order to accurately predict fuel ignition potential, fuel moisture content must be assessed by evaluating fire spread, fireline intensity and fuel consumption. Our objective here is to model moisture content of surface fuels in normally stocked Calabrian pine (Pinus brutia Ten.) stands in relation to weather conditions, namely temperature, relative humidity, and wind speed in the Mugla province of Turkey. All surface fuels were categorized according to diameter classes and fuel types. Six fuel categories were defined: these were 0\textendash 0.3, 0.3\textendash 0.6, and 0.6\textendash 1~cm diameter classes, and cone, surface litter, and duff. Plastic containers 15\,\texttimes\,20~cm in size with 1\,\texttimes\,1~mm mesh size were used. Samples were taken from 09:00 to 19:00~h and weighed every 2~h with 0.01~g precision for 10~days in August. At the end of the study, samples were taken to the laboratory, oven-dried at 105~\textdegree C for 24~h and weighed to obtain fuel-moisture contents. Weather measurements were taken from a fully automated weather station set up at the study site prior to the study. Correlation and regression analyses were carried out and models were developed to predict fuel moisture contents for desorption and adsorption phase for each fuel type categories. Practical fuel moisture prediction models were developed for dry period. Models were developed that performed well with reasonable accuracy, explaining up to 92 and 95.6\% of the variability in fuel-moisture contents for desorption and adsorption phases, respectively. Validation of the models were conducted using an independent data set and known fuel moisture prediction models. The predictive power of the models was satisfactory with mean absolute error values being 1.48 and 1.02 for desorption and adsorption as compared to the 2.05 and 1.60 values for the Van Wagner's hourly litter moisture content prediction model. Results obtained in this study will be invaluable for fire management planning and modeling.},
  langid = {english},
  keywords = {fire management,fuel moisture},
  file = {C\:\\Users\\walki\\Zotero\\storage\\D7CFV69W\\Bilgili et al_2019_Modeling surface fuels moisture content in Pinus brutia stands.pdf}
}

@article{bishopClassificationPlotlevelFirecaused2014,
  title = {Classification of Plot-Level Fire-Caused Tree Mortality in a Redwood Forest Using Digital Orthophotography and {{LiDAR}}},
  author = {Bishop, Brian D. and Dietterick, Brian C. and White, Russell A. and Mastin, Tom B.},
  year = {2014},
  month = mar,
  journal = {Remote Sensing},
  volume = {6},
  number = {3},
  pages = {1954--1972},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  issn = {2072-4292},
  doi = {10.3390/rs6031954},
  url = {https://www.mdpi.com/2072-4292/6/3/1954},
  urldate = {2022-10-21},
  abstract = {Aerial and satellite imagery are widely used to assess the severity and impact of wildfires. Light detection and ranging (LiDAR) is a newer remote sensing technology that has demonstrated utility in measuring vegetation structure. Combined use of imagery and LiDAR may improve the assessment of wildfire impacts compared to imagery alone. Estimation of tree mortality at the plot scale could serve for more rapid, broad-scale, and lower cost post-fire assessments than feasible through field assessment. We assessed the accuracy of classifying color-infrared imagery in combination with post-fire LiDAR, and with differenced (pre- and post-fire) LiDAR, in estimating plot percent mortality in a second-growth coast redwood forest near Santa Cruz, CA. Percent mortality of trees greater than 25.4 cm DBH in 47 permanent 0.08 ha plots was categorized as low ({$<$}25\%), moderate (25\%\textendash 50\%), or high ({$>$}50\%). The model using Normalized Difference Vegetation Index (NDVI) from National Agricultural Imagery Program (NAIP) was 74\% accurate; the model using NDVI and post-fire LiDAR was 85\% accurate, while the model using NDVI and differenced LiDAR was 83\% accurate. The addition of post-fire LiDAR data provided a modest increase in accuracy compared to imagery alone, which may not justify the substantial cost of data acquisition. The method demonstrated could be applied to rapidly estimate tree mortality resulting from wildfires at fine to moderate scale.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  langid = {english},
  keywords = {fire effects,lidar,tree mortality},
  file = {C\:\\Users\\walki\\Zotero\\storage\\U4F6UX6U\\Bishop et al_2014_Classification of Plot-Level Fire-Caused Tree Mortality in a Redwood Forest.pdf;C\:\\Users\\walki\\Zotero\\storage\\NCS426QJ\\1954.html}
}

@article{bonnerInvigoratingPrescribedFire2021,
  title = {Invigorating {{Prescribed Fire Science Through Improved Reporting Practices}}},
  author = {Bonner, Sophie R. and Hoffman, Chad M. and Kane, Jeffrey M. and Varner, J. Morgan and Hiers, J. Kevin and O'Brien, Joseph J. and Rickard, Heather D. and Tinkham, Wade T. and Linn, Rodman R. and Skowronski, Nicholas and Parsons, Russell A. and Sieg, Carolyn H.},
  year = {2021},
