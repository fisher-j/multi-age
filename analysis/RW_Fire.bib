@article{knappBehaviourEffectsPrescribed2011,
  title = {Behaviour and Effects of Prescribed Fire in Masticated Fuelbeds},
  author = {Knapp, Eric E. and Varner, J. Morgan and Busse, Matt D. and Skinner, Carl N. and Shestak, Carol J.},
  year = {2011},
  month = oct,
  journal = {International Journal of Wildland Fire},
  volume = {20},
  number = {8},
  pages = {932--945},
  publisher = {{CSIRO PUBLISHING}},
  issn = {1448-5516, 1448-5516},
  doi = {10.1071/WF10110},
  url = {https://www.publish.csiro.au/wf/WF10110},
  urldate = {2022-11-12},
  abstract = {Mechanical mastication converts shrub and small tree fuels into surface fuels, and this method is being widely used as a treatment to reduce fire hazard. The compactness of these fuelbeds is thought to moderate fire behaviour, but whether standard fuel models can accurately predict fire behaviour and effects is poorly understood. Prescribed burns were conducted in young ponderosa pine (Pinus ponderosa Laws.) forests at two sites in northern California where the midstorey layer dominated by shrubs had been masticated. Surface fuels were raked from the base of a subset of trees before burning. Rate of spread and flame length were estimated for both backing and heading fires, soil heating measured with thermocouples and tree fire injury recorded. Standard fuel models often over-predicted rate of spread or under-predicted flame length. Custom models generally provided a better balance between the slow rates of spread and moderate flame lengths observed in prescribed burns. Post-fire tree mortality was most strongly associated with crown scorch and tree size; raking fuels from the base of trees did not improve survival. Under severe fire weather conditions, fire behaviour and effect models as well as observations from wildfires suggest that mastication may be more effective for moderating fire behaviour than reducing residual tree mortality. Treating masticated fuels with prescribed burns could potentially improve the resilience of stands to wildfire.},
  langid = {english},
  keywords = {prescribed fire},
  file = {C\:\\Users\\walki\\Zotero\\storage\\FIL358F3\\Knapp et al_2011_Behaviour and effects of prescribed fire in masticated fuelbeds.pdf;C\:\\Users\\walki\\Zotero\\storage\\226FLLQF\\WF10110.html}
}

