@article{ciceuModelingDiameterDistribution2021,
  title = {Modeling the Diameter Distribution of Mixed Uneven-Aged Stands in the South Western {{Carpathians}} in {{Romania}}},
  author = {Ciceu, Albert and Pitar, Diana and Badea, Ovidiu},
  year = {2021},
