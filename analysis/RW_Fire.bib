@article{michaletzFoliageInfluencesForced2006,
  title = {Foliage Influences Forced Convection Heat Transfer in Conifer Branches and Buds},
  author = {Michaletz, S. T. and Johnson, E. A.},
  year = {2006},
  journal = {New Phytologist},
  volume = {170},
  number = {1},
