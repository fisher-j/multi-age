@article{alexanderCrownFireBehavior2013,
  title = {Crown Fire Behavior Characteristics and Prediction in Conifer Forests: A State-of-Knowledge Synthesis},
  author = {Alexander, Martin E and Cruz, Miguel G and Vaillant, Nicole M and Peterson, David L},
  year = {2013},
  pages = {39},
  abstract = {Joint Fire Science Program (JFSP) project 09-S-03-1 was undertaken in response to JFSP Project Announcement No. FA-RFA09-0002 with respect to a synthesis on extreme fire behavior or more specifically a review and analysis of the literature dealing with certain features of crown fire behavior in conifer forests in the United States and adjacent regions of Canada. The key findings presented are organized along nine topical areas: types of crown fires; crown fire initiation; crown fire propagation; crown fire rate of spread; crown fire intensity and flame zone characteristics; crown fire area and perimeter growth; crown fire spotting activity; models, systems, and other decision aids for predicting crown fire behavior; and implications for fire and fuel management. A total of 16 management implications are discussed at some length, involving the following subjects: \textbullet{} Classification of crown fires \textbullet{} Flames don't have to extend into the lower canopy for crowning to occur \textbullet{} Unsubstantiated coupling of crown fire behavior models \textbullet{} Defining canopy fuel stratum characteristics \textbullet{} Evaluating models to predict canopy fuel stratum characteristics \textbullet{} The myth of the ``crown fire-proof'' conifer forest \textbullet{} Lack of physics-based model evaluation in predicting crown fire behavior \textbullet{} Van Wagner's criteria for active crown fire spread is a robust concept \textbullet{} Foliar moisture content has little or no effect on crown fire rate of spread \textbullet{} Surface fire versus crown fire rates of spread prediction \textbullet{} An example of linking surface and crown fire behavior to fire effects \textbullet{} Model of elliptical crown fire length-to-breadth ratio underpredicts \textbullet{} Maximum spotting distance model for active crown fires \textbullet{} Reviews on predicting crown fire and wildland fire behavior have proven valuable \textbullet{} Alternative models for predicting the characteristics of crown fire behavior \textbullet{} Evaluation of fuel treatment effectiveness From the standpoint of relationships to other recent findings on the topic of crown fire behavior in conifer forests, four areas of ongoing work were identified: \textbullet{} Crown fire potential in mountain pine beetle-attack lodgepole pine forests \textbullet{} Physics-based fire behavior models 4 \textbullet{} Crown fire potential in other forest types \textbullet{} Experimental crown fires From a practical point of view, future work is needed in the following areas: \textbullet{} Systematic documentation of crowning wildfires for model evaluation purposes \textbullet{} Development and testing of flame size model for crown fires \textbullet{} Defining the threshold for vertical fire spread in terms of ladder/bridge fuels and canopy bulk density \textbullet{} Crown fuel consumption data collection and model evaluation \textbullet{} Application of ensemble or multiple simulation methods to the prediction of crown fire behavior The following type/number of deliverables emanated from Project JFSP 09-S-03-1: \textbullet{} Book chapters \textendash{} 5 \textbullet{} Special issue of Fire Management Today - 1 \textbullet{} Journal articles \textendash{} 16 \textbullet{} Conference papers and technical journal notices - 13 \textbullet{} Software \textendash{} 2 \textbullet{} Workshop \textendash{} 1 \textbullet{} Other contributions \textendash{} 7 \textbullet{} Websites \textendash{} 2 Collectively, the references associated with the book chapters and journal articles, constitutes a comprehensive bibliography on the subject of crown fire behavior in conifer forests.},
  langid = {english},
  keywords = {crown fire},
  file = {C\:\\Users\\walki\\Zotero\\storage\\I6WZA9CN\\Dr et al. - Crown fire behavior characteristics and prediction.pdf}
}

@article{alexanderEvaluatingModelPredicting2006,
  title = {Evaluating a Model for Predicting Active Crown Fire Rate of Spread Using Wildfire Observations},
