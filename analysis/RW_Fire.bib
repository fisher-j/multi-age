@article{packalenEstimationSpeciesspecificDiameter2008,
  title = {Estimation of Species-Specific Diameter Distributions Using Airborne Laser Scanning and Aerial Photographs},
  author = {Packal{\'e}n, Petteri and Maltamo, Matti},
