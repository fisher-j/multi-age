  abstract = {Biological\textendash mathematical models of trees can be exploited for a wide range of agronomic applications including crop management, visualization of ecosystem changes over time, in-field phenotyping, crop load effects, testing of plant functions, biomechanics, and many others. Some models propose a 3D output of tree that, in addition to having functionality to visualize the result, offers an additional tool for the evaluation of some parameters of the model itself (interception and amount of light, temperature, obstacles, physical competition between multiple trees). The present study introduces a biological\textendash mathematical model of tree growth with a 3D output of its structure in a realtime 3D rendering environment (Unity\textcopyright ). Thanks to the virtual environment created in Unity\textcopyright, it was possible to obtain variable environmental parameters (amount of light, temperature) used as inputs to the mathematical simulation of growth. The model is based on ordinary differential equations (ODEs) that compute the growth of each single internode in length (primary growth) and width (secondary growth) and the accumulation of growth inhibitors regulating the seasonal cyclicity of the tree. Virtual experiments were conducted varying environmental conditions (amount of light and temperature), and the species-specific characteristics of the simulated tree (number of buds, branching angle). The results have been analyzed showing also how the model can be adapted for the creation of different tree species and discussing the potential agronomic applications of model.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  langid = {english},
  keywords = {plant model},
  file = {C\:\\Users\\walki\\Zotero\\storage\\865VHZIY\\Crimaldi et al_2023_Integration of a System Dynamics Model and 3D Tree Rendering—VISmaF Part II.pdf}
}

@article{crimaldiVISmaFSyntheticTree2021,
  title = {{{VISmaF}}: {{Synthetic Tree}} for {{Immersive Virtual Visualization}} in {{Smart Farming}}. {{Part I}}: {{Scientific Background Review}} and {{Model Proposal}}},
  shorttitle = {{{VISmaF}}},
  author = {Crimaldi, Mariano and Carten{\`i}, Fabrizio and Giannino, Francesco},
  year = {2021},
